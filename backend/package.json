{
  "name": "server_ts",
  "version": "1.0.0",
  "description": "",
  "main": "/src/server.ts",
  "scripts": {
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "test:coverage:json": "nyc --reporter=json-summary npm run test",
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./build/server.js",
    "predev": "npm run db:reset",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "lint": "eslint ./src --ext .ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/chai": "^4.3.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "16.11",
    "@types/sinon": "^17.0.3",
    "bcryptjs": "^2.4.3",
    "chai": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.4.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.4"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jest": "^29.7.0",
    "joi": "^17.12.3",
    "mysql2": "^3.9.3",
    "nodemon": "^3.1.0",
    "sequelize": "^6.37.2",
    "sequelize-cli": "^6.6.2"
  }
}
